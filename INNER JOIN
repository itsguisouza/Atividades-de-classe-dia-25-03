CREATE TABLE CIDADES (
  ID INT PRIMARY KEY,
  NOME VARCHAR(60) NOT NULL,
  POPULACAO INT
  );
  
CREATE TABLE ALUNOS (
  ID INT PRIMARY KEY,
  NOME VARCHAR(60) NOT NULL,
  DATA_NASC DATE,
  CIDADE_ID INT,
  
  FOREIGN KEY(CIDADE_ID) REFERENCES CIDADE(ID)
  );
  
